{
  "name": "potler",
  "version": "0.1.0",
  "scripts": {
    "migrate": "npx ts-node src/migrate.ts",
    "gen": "kysely-codegen --out-file src/@types/kysely.d.ts",
    "dev": "nodemon",
    "build": "npx tsc && tsc-alias",
    "prod": "node -r tsconfig-paths/register dist/index.js"
  },
  "nodemonConfig": {
    "watch": [ "src" ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@node-rs/argon2": "^1.5.2",
    "better-sqlite3": "^9.2.2",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-jwt": "^8.4.1",
    "helmet": "^7.1.0",
    "jet-logger": "^1.3.1",
    "jet-paths": "^1.0.6",
    "jet-validator": "^1.1.1",
    "kysely": "^0.26.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "sharp-multer": "^0.3.2",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.22.4",
    "zod-express-middleware": "^1.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@types/better-sqlite3": "^7.6.8",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "kysely-codegen": "^0.11.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3"
  }
}
